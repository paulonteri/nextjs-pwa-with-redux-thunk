{
    "name": "nextjs-redux-thunk-template",
    "version": "0.1.0",
    "scripts": {
        "dev": "next",
        "build": "yarn && yarn next build && yarn next export",
        "start": "next start",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run --headless --browser chrome",
        "test": "yarn cypress:run",
        "pretty": "prettier --write \"src/**/*.{js,jsx}\""
    },
    "dependencies": {
        "next": "latest",
        "next-pwa": "^5.0.4",
        "nprogress": "^0.2.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-redux": "^7.1.0",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "cypress": "^6.3.0",
        "husky": "^4.3.8",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1"
    },
    "lint-staged": {
        "*.js": [
            "prettier --write",
            "git add"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "license": "MIT"
}
